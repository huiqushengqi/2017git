package com.etc.painter.entity;
//公司活动实体类
public class Activities {
private int act_id;
private String act_item;
private String act_time;
private String act_addr;
private String act_content;
private String act_img;
public int getAct_id() {
	return act_id;
}
public void setAct_id(int act_id) {
	this.act_id = act_id;
}
public String getAct_item() {
	return act_item;
}
public void setAct_item(String act_item) {
	this.act_item = act_item;
}
public String getAct_time() {
	return act_time;
}
public void setAct_time(String act_time) {
	this.act_time = act_time;
}
public String getAct_addr() {
	return act_addr;
}
public void setAct_addr(String act_addr) {
	this.act_addr = act_addr;
}
public String getAct_content() {
	return act_content;
}
public void setAct_content(String act_content) {
	this.act_content = act_content;
}
public String getAct_img() {
	return act_img;
}
public void setAct_img(String act_img) {
	this.act_img = act_img;
}
public Activities(int act_id, String act_item, String act_time,
		String act_addr, String act_content, String act_img) {
	super();
	this.act_id = act_id;
	this.act_item = act_item;
	this.act_time = act_time;
	this.act_addr = act_addr;
	this.act_content = act_content;
	this.act_img = act_img;
}
public Activities() {
	super();
}

}
