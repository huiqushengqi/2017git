package com.etc.painter.entity;
//员工表
public class Manager {
private Integer mag_id;
private String mag_name;
private String mag_job;//职位
private String mag_about;//简历
private String mag_img;//图片
private String mag_date;//时间

public String getMag_date() {
	return mag_date;
}
public void setMag_date(String magDate) {
	mag_date = magDate;
}
public void setMag_id(Integer magId) {
	mag_id = magId;
}
public int getMag_id() {
	return mag_id;
}
public void setMag_id(int mag_id) {
	this.mag_id = mag_id;
}
public String getMag_name() {
	return mag_name;
}
public void setMag_name(String mag_name) {
	this.mag_name = mag_name;
}
public String getMag_job() {
	return mag_job;
}
public void setMag_job(String mag_job) {
	this.mag_job = mag_job;
}
public String getMag_about() {
	return mag_about;
}
public void setMag_about(String mag_about) {
	this.mag_about = mag_about;
}
public String getMag_img() {
	return mag_img;
}
public void setMag_img(String mag_img) {
	this.mag_img = mag_img;
}
public Manager(Integer magId, String magName, String magJob, String magAbout,
		String magImg, String magDate) {
	super();
	mag_id = magId;
	mag_name = magName;
	mag_job = magJob;
	mag_about = magAbout;
	mag_img = magImg;
	mag_date = magDate;
}
public Manager() {
	super();
}

}
