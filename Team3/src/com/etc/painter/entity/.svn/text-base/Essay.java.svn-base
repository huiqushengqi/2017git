package com.etc.painter.entity;

public class Essay {

	private Integer essay_id;
	private String essay_title;
	private String essay_content;
	private String essay_time;
	private User essay_user;
	private int essay_check;

	
	
	public int getEssay_check() {
		return essay_check;
	}
	public void setEssay_check(int essayCheck) {
		essay_check = essayCheck;
	}
	public Integer getEssay_id() {
		return essay_id;
	}
	public void setEssay_id(Integer essayId) {
		essay_id = essayId;
	}
	public String getEssay_title() {
		return essay_title;
	}
	public void setEssay_title(String essayTitle) {
		essay_title = essayTitle;
	}
	public String getEssay_content() {
		return essay_content;
	}
	public void setEssay_content(String essayContent) {
		essay_content = essayContent;
	}
	
	public String getEssay_time() {
		return essay_time;
	}
	public void setEssay_time(String essayTime) {
		essay_time = essayTime;
	}
	public User getEssay_user() {
		return essay_user;
	}
	public void setEssay_user(User essayUser) {
		essay_user = essayUser;
	}
	public Essay(Integer essayId, String essayTitle, String essayContent,
			String time, User essayUser,int essayCheck) {
		super();
		essay_id = essayId;
		essay_title = essayTitle;
		essay_content = essayContent;
		essay_time = time;
		essay_user = essayUser;
		essay_check=essayCheck;
	}
	public Essay() {
		super();
	}
	@Override
	public String toString() {
		return "Essay [essay_check=" + essay_check + ", essay_content="
				+ essay_content + ", essay_id=" + essay_id + ", essay_time="
				+ essay_time + ", essay_title=" + essay_title + ", essay_user="
				+ essay_user + "]";
	}

	
	
}
