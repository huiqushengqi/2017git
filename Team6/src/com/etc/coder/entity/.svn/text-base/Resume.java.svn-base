package com.etc.coder.entity;

public class Resume {
	private int resume_id;// 简历id
	private String resume_name; //简历名称  
	private int resume_basic;//用户基本信息  ，外键
	private int resume_edu;//用户的教育背景，外键
	private int resume_work;//用户的工作经历,外键
	private int resume_job;//用户的求职意向，外键
	private int resume_project;//用户的项目经验，外键
	private String resume_user;//登录的用户名
	public String getResume_user() {
		return resume_user;
	}
	public void setResume_user(String resume_user) {
		this.resume_user = resume_user;
	}
	public int getResume_id() {
		return resume_id;
	}
	public void setResume_id(int resume_id) {
		this.resume_id = resume_id;
	}
	public String getResume_name() {
		return resume_name;
	}
	public void setResume_name(String resume_name) {
		this.resume_name = resume_name;
	}
	public int getResume_basic() {
		return resume_basic;
	}
	public void setResume_basic(int resume_basic) {
		this.resume_basic = resume_basic;
	}
	public int getResume_edu() {
		return resume_edu;
	}
	public void setResume_edu(int resume_edu) {
		this.resume_edu = resume_edu;
	}
	public int getResume_work() {
		return resume_work;
	}
	public void setResume_work(int resume_work) {
		this.resume_work = resume_work;
	}
	public int getResume_job() {
		return resume_job;
	}
	public void setResume_job(int resume_job) {
		this.resume_job = resume_job;
	}
	public int getResume_project() {
		return resume_project;
	}
	public void setResume_project(int resume_project) {
		this.resume_project = resume_project;
	}
	
	
	
	
}
