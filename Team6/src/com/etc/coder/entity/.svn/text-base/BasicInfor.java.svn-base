package com.etc.coder.entity;

public class BasicInfor {

	private int basic_id;	
	private String basic_name; //用户名
	private String basic_birthday;//用户出生日期
	private String basic_gender; //用户性别
	private String basic_idNumber; //用户身份证号
	private String basic_email; //用户邮箱
	private String basic_mobilePhone;//用户联系方式
	private String basic_state;  //当前状态
	private String basic_home;//具体的家庭住址
	private String basic_photo;//应聘者图片
	
	public int getBasic_id() {
		return basic_id;
	}
	public void setBasic_id(int basic_id) {
		this.basic_id = basic_id;
	}
	public String getBasic_name() {
		return basic_name;
	}
	public void setBasic_name(String basic_name) {
		this.basic_name = basic_name;
	}
	public String getBasic_birthday() {
		return basic_birthday;
	}
	public void setBasic_birthday(String basic_birthday) {
		this.basic_birthday = basic_birthday;
	}
	public String getBasic_gender() {
		return basic_gender;
	}
	public void setBasic_gender(String basic_gender) {
		this.basic_gender = basic_gender;
	}
	public String getBasic_idNumber() {
		return basic_idNumber;
	}
	public void setBasic_idNumber(String basic_idNumber) {
		this.basic_idNumber = basic_idNumber;
	}
	public String getBasic_email() {
		return basic_email;
	}
	public void setBasic_email(String basic_email) {
		this.basic_email = basic_email;
	}
	public String getBasic_mobilePhone() {
		return basic_mobilePhone;
	}
	public void setBasic_mobilePhone(String basic_mobilePhone) {
		this.basic_mobilePhone = basic_mobilePhone;
	}
	public String getBasic_state() {
		return basic_state;
	}
	public void setBasic_state(String basic_state) {
		this.basic_state = basic_state;
	}
	public String getBasic_home() {
		return basic_home;
	}
	public void setBasic_home(String basic_home) {
		this.basic_home = basic_home;
	}
	public String getBasic_photo() {
		return basic_photo;
	}
	public void setBasic_photo(String basic_photo) {
		this.basic_photo = basic_photo;
	}
	
	
}
